{
  "name": "mem-rag",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "mem-rag": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "mem-rag": "pnpm run build && node dist/cli.js",
