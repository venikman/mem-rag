    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.13.5",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^2.1.9"
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk": "1.26.0",
      "esbuild": "0.25.0"
    }
  }
